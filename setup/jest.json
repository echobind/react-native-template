{
  "preset": "react-native",
  "moduleFileExtensions": ["ts", "tsx", "js"],
  "transform": {
    "^.+\\.(js)$": "<rootDir>/node_modules/babel-jest",
    "\\.(ts|tsx)$": "ts-jest"
  },
  "testRegex": "(/__tests__/.*|(test|spec))\\.(ts|tsx|js)$",
  "testPathIgnorePatterns": ["\\.snap$", "e2e", "<rootDir>/node_modules/"],
  "cacheDirectory": ".jest/cache"
}
